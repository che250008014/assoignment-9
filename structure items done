#include<iostream>
using namespace std ;

struct Item {
    int id;
    float cost;
};

int main() {
    
    Item items[6] = {
        {101, 75.50},  
        {102, 45.00},    
        {103, 120.75},   
        {104, 30.25},    
    };

    std::cout << "Items with cost greater than 50.00:" << std::endl;
    for (int i = 0; i < 6; ++i) {
        if (items[i].cost > 50.00) {
            std::cout << "Item ID: " << items[i].id 
                      << ", Cost: " << items[i].cost << std::endl;
        }
    }

    return 0;
}
